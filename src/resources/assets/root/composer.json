{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "^1.0",
        "jenssegers/date": "^3.2",
        "guzzlehttp/guzzle": "~6.0",
        "doctrine/dbal": "^2.5",
        "pusher/pusher-php-server": "^2.5",
        "laravelcollective/html": "^5.4.1",
        "maatwebsite/excel": "^2.1.0",
        "laravel-enso/core": "^1.0.3",
        "laravel-enso/historytracker": "^1.0.0",
        "laravel-enso/commentsmanager": "^1.0.0",
        "laravel-enso/documentsmanager": "^1.0.0",
        "laravel-enso/localisation": "^1.0.2",
        "laravel-enso/logmanager": "^1.0.0",
        "laravel-enso/tutorialmanager": "^1.0.0",
        "laravel-enso/notifications": "^1.0.0",
        "laravel-enso/actionlogger": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "laracasts/generators": "dev-master",
        "phploc/phploc": "*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
