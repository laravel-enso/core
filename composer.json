        {
            "name": "Mihai Ocneanu",
            "email": "mihai.ocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        },
        {
            "name": "Ionut Pirvulescu",
            "email": "ionut.pirvulescu1@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel-enso/action-logger": "^3.1",
        "laravel-enso/avatars": "^4.0",
        "laravel-enso/charts": "^4.0",
        "laravel-enso/companies": "^4.0",
        "laravel-enso/data-export": "^3.0",
        "laravel-enso/dynamic-methods": "^2.0",
        "laravel-enso/enums": "^2.0",
        "laravel-enso/files": "^5.0",
        "laravel-enso/forms": "^4.0",
        "laravel-enso/helpers": "^2.0",
        "laravel-enso/history-tracker": "^2.0",
        "laravel-enso/impersonate": "^3.2",
        "laravel-enso/io": "^2.3",
        "laravel-enso/localisation": "^5.0",
        "laravel-enso/logs": "^4.0",
        "laravel-enso/migrator": "^2.0",
        "laravel-enso/menus": "^5.0",
        "laravel-enso/notifications": "^4.1",
        "laravel-enso/people": "^4.0",
        "laravel-enso/permissions": "^5.0",
        "laravel-enso/rememberable": "^3.0",
        "laravel-enso/roles": "^5.0",
        "laravel-enso/searchable": "^2.2",
        "laravel-enso/sentry": "^1.0",
        "laravel-enso/select": "^4.0",
        "laravel-enso/track-who": "^2.0",
        "laravel-enso/tables": "^4.0",
        "laravel-enso/upgrade": "^2.5",
        "laravel-enso/users": "^2.0",
        "voku/anti-xss": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Core\\": "src/",
            "LaravelEnso\\Core\\Database\\Factories\\": "database/factories/",
            "LaravelEnso\\Core\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Core\\AppServiceProvider",
                "LaravelEnso\\Core\\BroadcastServiceProvider",
                "LaravelEnso\\Core\\EventServiceProvider",
                "LaravelEnso\\Core\\PasswordServiceProvider",
                "LaravelEnso\\Core\\MiddlewareServiceProvider",
                "LaravelEnso\\Core\\WebsocketServiceProvider"
            ]
        }
    }
}