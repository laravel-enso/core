{
    "name": "laravel-enso/core",
    "description": "Main requirement & dependency aggregator for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "enso-core",
        "laravel-boilerplate",
        "vue-bulma",
        "vue-spa"
    ],
    "homepage": "https://github.com/laravel-enso/Core",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        },
        {
            "name": "Mihai Ocneanu",
            "email": "mihai.ocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        },
        {
            "name": "Ionut Pirvulescu",
            "email": "ionut.pirvulescu1@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.7.*",
        "guzzlehttp/guzzle": "~6.0",
        "jenssegers/date": "^3.4",
        "laravel-enso/actionlogger": "2.2.*",
        "laravel-enso/activityLog": "1.1.*",
        "laravel-enso/avatarmanager": "2.3.*",
        "laravel-enso/charts": "2.5.*",
        "laravel-enso/companies": "1.2.*",
        "laravel-enso/dataexport": "^1.1",
        "laravel-enso/dataimport": "3.0.*",
        "laravel-enso/filemanager": "2.5.*",
        "laravel-enso/formbuilder": "2.12.*",
        "laravel-enso/helpers": "1.10.*",
        "laravel-enso/historytracker": "1.4.*",
        "laravel-enso/impersonate": "2.3.*",
        "laravel-enso/localisation": "2.4.*",
        "laravel-enso/logmanager": "2.2.*",
        "laravel-enso/menumanager": "2.3.*",
        "laravel-enso/notifications": "2.2.*",
        "laravel-enso/people": "1.1.*",
        "laravel-enso/permissionmanager": "2.3.*",
        "laravel-enso/rememberable": "1.4.*",
        "laravel-enso/rolemanager": "2.5.*",
        "laravel-enso/searchable": "1.0.*",
        "laravel-enso/structuremanager": "2.5.*",
        "laravel-enso/trackwho": "1.7.*",
        "laravel-enso/tutorialmanager": "2.2.*",
        "laravel-enso/versioning": "1.1.*",
        "laravel-enso/vuecomponents": "2.7.*",
        "laravel-enso/vuedatatable": "1.9.*",
        "laravel/horizon": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Core\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Core\\AppServiceProvider",
                "LaravelEnso\\Core\\AuthServiceProvider",
                "LaravelEnso\\Core\\EventServiceProvider"
            ]
        }
    }
}
